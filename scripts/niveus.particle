			accelerationType		static
			accelerationDir			linear
			acceleration	 		0 0 -1 ~25
			accelerationMagnitude 	300

			bounce		0
			radius		0 1 1
			alpha		0 .5 .0
			rotation	0 0 ~360

			lifeTime 600

			overdrawProtection
		}
		particle
		{
		shader sync textures/niveus/spark_02_s

		displacement 0 0 0 ~5

			velocityType 		static
			velocityDir 		linear
			velocity 			0 0 -1 ~150
			velocityMagnitude 	100

			accelerationType		static
			accelerationDir			linear
			acceleration			0 0 -1 ~25
			accelerationMagnitude 	300

			bounce		0
			radius		0 1 1
			alpha		0 .5 .0
			rotation	0 0 ~360

			lifeTime 400

			overdrawProtection
		}
		particle
		{
			shader sync textures/niveus/spark_02_s

			displacement 0 0 0 ~5

			velocityType 		static
			velocityDir 		linear
			velocity 			0 0 -1 ~150
			velocityMagnitude 	100

			accelerationType		static
			accelerationDir			linear
			acceleration			0 0 -1 ~25
			accelerationMagnitude 	300

			bounce		0
			radius		0 1 1
			alpha		0 .5 .0
			rotation	0 0 ~360

			lifeTime 500

			overdrawProtection
		}
		count infinite
		delay 0
		period 1500 - ~100%
	}
}

niveus/bigSteamerDown
{
	ejector
	{
		particle
		{
			shader sync textures/niveus/mist_s
