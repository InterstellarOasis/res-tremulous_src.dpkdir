niveus/sparkerDown
{
	ejector
	{
		particle
		{
			shader sync textures/niveus/spark_02_s

			displacement 0 0 0 ~3

			velocityType 		static
			velocityDir 		linear
			velocity 			0 0 -1 ~150
			velocityMagnitude 	100

			accelerationType		static
			accelerationDir			linear
			acceleration			0 0 -1 ~25
			accelerationMagnitude 	300

			bounce		0
			radius		0 1 1
			alpha		0 .5 .0
			rotation	0 0 ~360

			lifeTime 300

			overdrawProtection
		}
		particle
		{
			shader sync textures/niveus/spark_02_s

			displacement 0 0 0 ~5

			velocityType 		static
			velocityDir 		linear
			velocity 			0 0 -1 ~150
